
<%- include('header') -%>
    <div class="box">
      <% newListItem.forEach((item => { %>
      <div class="item">
        <input type="checkbox" />
        <p><%=item%></p>
      </div>
      <% }))%>

      <form class="item" action="/" method="post">
        <input
          type="text"
          name="newItem"
          placeholder="New Item"
          autocomplete="off"
        />
        <!-- prettier-ignore -->
        <button type="submit" name="list" value= <%=listTitle%> >+</button>
      </form>
    </div>
  </body>
</html>
